    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^7.0.4",
    "immer": "^9.0.21",
    "lint-staged": "^11.2.6",
    "mkdirp": "^1.0.4",
    "prettier": "^2.8.8",
    "prop-types": "^15.8.1",
