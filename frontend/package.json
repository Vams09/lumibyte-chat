    "react-dom": "^19.2.0",
    "react-helmet-async": "^1.3.0",
    "react-redux": "^7.2.9",
    "react-router-dom": "^6.30.2",
    "react-scripts": "5.0.1",
    "redux": "^4.2.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-mock-store": "^1.5.5",
    "redux-observable": "^2.0.0",
